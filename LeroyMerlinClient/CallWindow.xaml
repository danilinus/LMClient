        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeroyMerlinClient"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Обзвон" Height="313" Width="314">
    <Grid>
        <Label x:Name="Nomber" Margin="10,10,10,0" VerticalAlignment="Top" Height="28" Width="272" Content="Номер:"/>
        <ScrollViewer Margin="17,43,24,46" VerticalScrollBarVisibility="Auto">
            <TextBlock x:Name="TextObz" TextWrapping="Wrap"/>
        </ScrollViewer>
        <Button Content="OK" HorizontalAlignment="Right" Margin="0,0,17,21" VerticalAlignment="Bottom" Width="75" Background="#7F9ACD32" BorderBrush="YellowGreen" Click="Button_Click"/>
    </Grid>
</Window>
